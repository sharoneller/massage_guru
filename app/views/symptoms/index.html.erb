<header id="content-header">
  <h2>Symptoms Relieved by <%= link_to(@recommendation.modality, @recommendation) %></h2>
</header>
<ul>
  <% @symptoms.each do |symptom| %>
	<li>
		<strong><%= symptom.name %> with a severity of <%= symptom.severity %> (1 - 10 scale)</strong>
			
	</li>
  <% end %>
</ul>